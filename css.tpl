#{{uc_id}} .testimonial_slider_content{
  background: #FFFFFF;
  /* Level 2 */
  box-shadow: 0px 5px 15px rgba(0, 176, 244, 0.15);
  border-radius: 5px;
  margin: 0 15px;
}
#{{uc_id}} .header_testimonial{
  padding: 40px 30px 60px;
}
#{{uc_id}} .footer_testimonial{
  background: #38D430;
  border-radius: 0px 0px 5px 5px;
  padding: 60px 30px 40px;
  position: relative;
}
#{{uc_id}} .testimonial_slider_content_image{
  position: absolute;
  left: 30px;
  top: -40px;
}
#{{uc_id}} .testimonial_slider_content_title p{
  font-weight: 700;
  font-size: 16px;
  line-height: 24px;
  color: #171717;
  margin: 0;
}
#{{uc_id}} .slick-dots li button {
  font-size: 0;
  line-height: 0;
  display: block;
  width: 20px;
  height: 20px;
  padding: 5px;
  cursor: pointer;
  color: transparent;
  border: 0;
  outline: none;
  background: #E6E6E6;
  border-radius: 2px;
}
#{{uc_id}} .slick-dots li button:before{
  color: transparent!important;
}
#{{uc_id}} .slick-dots li.slick-active button {
  font-size: 0;
  line-height: 0;
  display: block;
  width: 20px;
  height: 20px;
  padding: 5px;
  cursor: pointer;
  color: transparent;
  border: 0;
  outline: none;
  background: #171717;
  border-radius: 2px;
}
#{{uc_id}} .slick-dots{
  position: absolute;
  bottom: -55px;
  display: block;
  width: 100%;
  padding: 0;
  margin: 0;
  list-style: none;
  text-align: center;
}

@media (max-width: 768px){
#{{uc_id}} .slick-dots{
  position: absolute;
  bottom: -55px!important;
  display: block;
  width: 100%;
  padding: 0;
  margin: 0;
  list-style: none;
  text-align: center;
}
}