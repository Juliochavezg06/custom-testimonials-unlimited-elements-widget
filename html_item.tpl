<div class="testimonial_slider_content">
  <div class="header_testimonial">
    <div class="testimonial_slider_content_icon">
      <img src="https://public.geeksxperience.com/wp-content/uploads/2023/06/2.svg" alt="quote">
    </div>
    <div class="testimonial_slider_description">
      <p>{{item.blockquote|raw}}</p>
    </div>
  </div>
  <div class="footer_testimonial">
    <div class="testimonial_slider_content_image">
      <img src="{{item.image_thumb_large}}" alt="{{item.image_alt}}">
    </div>
    <div class="testimonial_slider_content_title">
      <p>{{item.title}}</p>
    </div>
  </div>
</div>